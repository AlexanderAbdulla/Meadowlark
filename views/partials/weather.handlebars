<div class="weatherWidget">
    {{#each partials.weather.locations}}
    <div class="location">
        <h3>
            {{name}}
        </h3>
        <a href="{{forecastUrl}}">
            <img src="{{iconUrl}}">
            {{weather}}, {{temp}}
        </a>
    </div>
    {{/each}}
    <small>Source <a href="http://wunderground.com">Weather Underground</a></small>
</div>